$webResourceManager.requireResource("de.uhd.ifi.se.decision.management.jira:stylesheet-and-icon-resources")
$webResourceManager.requireResource("de.uhd.ifi.se.decision.management.jira:decision-knowledge-management-resources")
$webResourceManager.requireResource("de.uhd.ifi.se.decision.management.jira:release-note-page-resources")
<div>
<h1>release Notes</h1>
    <button onclick="addReleaseNote()">add</button>
</div>

#parse("templates/dialogs/createReleaseNoteDialog.vm")

<script>
/*
 * Instantiate the ConDecKnowledgePage object controlling this view:
 */
$(document).ready(function() {
    console.log("conDecReleaseNotePage working")

    conDecAPI.checkIfProjectKeyIsValid();
    if ( window.conDecReleaseNotePage.init(conDecAPI, conDecObservable, conDecDialog, conDecContextMenu, conDecTreant, conDecTreeViewer) )
        window.conDecReleaseNotePage.getReleaseNotes();
    else {
        console.error("conDecReleaseNotePage could not get its dependencies.");
    }

});
function addReleaseNote(){
    window.conDecReleaseNotePage.addReleaseNote();
}
</script>
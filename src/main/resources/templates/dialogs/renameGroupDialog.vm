<section id="rename-group-dialog" class="aui-dialog2 aui-dialog2-medium aui-layer" role="dialog" aria-hidden="true">
    <header class="aui-dialog2-header">
        <h2 id="rename-group-dialog-header" class="aui-dialog2-header-main">$i18n.getText(
            "condec.context.menu.group.rename.label")</h2>
        <a class="aui-dialog2-header-close">
            <span class="aui-icon aui-icon-small aui-iconfont-close-dialog">Close</span>
        </a>
    </header>
    <div id="rename-group-dialog-content" class="aui-dialog2-content">
        <form class="aui">
            <div class="field-group">
                <label for="rename-group-input">Rename Group To:</label>
                <input id="rename-group-input" class="text full-width-field" value=""/>
            </div>
        </form>
    </div>
    <footer class="aui-dialog2-footer">
        <div class="aui-dialog2-footer-actions">
            <button id="rename-group-dialog-submit-button" class="aui-button aui-button-primary">Confirm</button>
            <button id="rename-group-dialog-cancel-button" class="aui-button aui-button-link">$i18n.getText(
                "condec.button.cancel")</button>
        </div>
    </footer>
</section>
<script>
/*
 * Instantiate the ConDecKnowledgePage object controlling this view:
 */
$(document).ready(function() {
	conDecAPI.checkIfProjectKeyIsValid();
    if ( window.conDecKnowledgePage.init(conDecAPI, conDecObservable, conDecDialog, conDecContextMenu, conDecTreant, conDecTreeViewer, window) )

<div class="quality-check-container">
    <div class="text-field">
        <label for="condec-tab-minimum-coverage-$viewIdentifier">$i18n.getText("condec.label.quality.check.minimum.coverage")</label>
        <label id="condec-tab-minimum-coverage-$viewIdentifier"></label>
    </div>
    <div class="text-field">
        <label for="condec-tab-link-distance-$viewIdentifier">$i18n.getText("condec.label.quality.check.link.distance")</label>
        <label id="condec-tab-link-distance-$viewIdentifier"></label>
    </div>
    <div class="text-field" id="quality-check-issue-text-$viewIdentifier">
    </div>
    <div class="text-field" id="quality-check-decision-text-$viewIdentifier">
    </div>
    <div class="text-field" id="quality-check-knowledge-complete-text-$viewIdentifier">
    </div>
    <div class="text-field" id="quality-check-knowledge-complete-criteria-text-$viewIdentifier">
    </div>
</div>

<div class="condec-prompt">
#parse("templates/prompts/definitionOfDoneCheckingPrompt.vm")
</div>

<script>
    /* Adds the on click listener on tab opening */
    $(document).ready(function () {
        conDecQualityCheck.initView("jira-issue-module");
    });
</script>
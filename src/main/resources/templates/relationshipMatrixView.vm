<div class='my-legend'>
    <div class='legend-title'>Relationships</div>
    <div class='legend-scale'>
        <ul class='legend-labels' id="legendList">
        </ul>
    </div>
</div>

<script>
    $(document).ready(function() {
        conDecAPI.checkIfProjectKeyIsValid();
        if(window.conDecRelationshipMatrixPage.init(conDecAPI, conDecObservable)) {
            conDecAPI.checkIfProjectKeyIsValid();
            var click = 0;
        	AJS.$("#menu-item-matrix").on("click", function(event) {
        		if (click === 0) {
        			window.conDecRelationshipMatrixPage.buildMatrix();
                    window.conDecRelationshipMatrixPage.buildLegend();
        		}
        		click++;
        	});  
        }
        else {
            console.error("ConDecRelationshipMatrixPage could not get its dependencies.");
        }
    });
</script>
<form class="aui">
	#set ($knowledgeTypeDropdownId = "knowledge-type-dropdown-comparison")
	#parse("templates/filter/knowledgeType.vm")

	#set ($statusDropdownId = "status-dropdown-comparison")	
    #parse("templates/filter/status.vm")

    #set ($textInputId = "compare-search-input")
    #parse("templates/filter/text.vm")

    #set ($filterButtonId = "filter-button-compare")
    #parse("templates/filter/filterButton.vm")
</form>
<div class="aui-group">
    <div  class="aui-item">
    	#set ($datepickerStartId = "start-data-picker-compare-left")
    	#set ($datepickerEndId = "end-data-picker-compare-left")
    	#parse("templates/filter/creationDate.vm")
        <div id="left-network"></div>
    </div>
    <div class="aui-item">
    	#set ($datepickerStartId = "start-data-picker-compare-right")
    	#set ($datepickerEndId = "end-data-picker-compare-right")
    	#parse("templates/filter/creationDate.vm")
        <div id="right-network"></div>
    </div>
</div>
<script>
    $(document).ready(function() {
        conDecAPI.checkIfProjectKeyIsValid();
        if(window.conDecEvolutionPage.init(conDecAPI, conDecObservable, conDecVis)) {
            conDecAPI.checkIfProjectKeyIsValid();
            window.conDecEvolutionPage.buildCompare();
        }
        else {
            console.error("ConDecEvolutionPage could not get its dependencies.");
        }
    });
</script>
	<web-resource key="classifier-resources"
		name="Resources for Decision Knowledge Extraction">
		<description>
			Provides the resources needed for extraction of decision knowledge from Jira issue
			comments such as the model files for Weka and Meka classifiers.
		</description>
		<context>jira.view.issue</context>
		<resource type="download" name="defaultTrainingData.arff"
			location="/classifier/defaultTrainingData.arff" />
		<resource type="download" name="condec.tab.panel.js" location="/js/condec.tab.panel.js" />

		<!--Language models -->
		<resource type="download" name="person.bin" location="/classifier/person.bin" />
		<resource type="download" name="lemmatizer.dict" location="/classifier/lemmatizer.dict" />
		<resource type="download" name="glove.6b.50d.csv" location="/classifier/glove.6b.50d.csv" />
		<resource type="download" name="pos.bin" location="/classifier/pos.bin" />
		<resource type="download" name="token.bin" location="/classifier/token.bin" />
	</web-resource>

	<!-- Decision Knowledge Page -->
	<web-item name="Link to the Decision Knowledge Page in Side Bar" key="sidebar-link"
		section="jira.project.sidebar.plugins.navigation" weight="1000">
		<description>Provides the "Decision Knowledge" link in the project's navigation sidebar.
		</description>
		<label key="condec.sidebar.link.label" />
		<param name="iconClass" value="aui-icon aui-icon-small aui-iconfont-decision" />
		<link>/projects/$pathEncodedProjectKey?selectedItem=decision-knowledge-page</link>
		<condition class="de.uhd.ifi.se.decision.management.jira.config.ActivationCondition" />
	</web-item>
	<web-panel name="Decision Knowledge Page" key="decision-knowledge-page"
		location="decision-knowledge-page" weight="1000">
		<description>
			Provides the plug-in page that developers use to manage decision knowledge.
		</description>
		<resource name="view" type="velocity" location="templates/decisionKnowledgePage.vm" />
	</web-panel>

	<!-- Release Notes Page -->
	<web-item name="Link to the Release Notes Page in Side Bar" key="sidebar-link-release-notes"
		section="jira.project.sidebar.plugins.navigation" weight="1000">
		<description>Provides the "Release Notes" link in the project's navigation sidebar.
		</description>
		<label key="condec.sidebar.link.label.release.notes" />
		<param name="iconClass" value="aui-icon aui-icon-small aui-iconfont-queues" />
		<link>/projects/$pathEncodedProjectKey?selectedItem=release-notes-page</link>
		<condition class="de.uhd.ifi.se.decision.management.jira.config.ActivationCondition" />
	</web-item>
	<web-panel name="Release Notes Page" key="release-notes-page"
		location="release-notes-page" weight="1000">
		<description>
			Provides the plug-in page that developers use to manage release notes.
		</description>
		<resource name="view" type="velocity"
			location="templates/releasenotes/releaseNotesPage.vm" />
	</web-panel>

	<!-- REST API -->
	<rest name="Decision Knowledge REST" key="decision-knowledge-rest" path="/condec"
		version="1.0">
		<description>
			Provides REST resources to manage decision knowledge.
		</description>
	</rest>
	<rest name="Release Note REST" key="release-note-rest" path="/release-note" version="1.0">
		<description>
			Provides REST resources to manage Release Notes.

	<web-resource key="release-note-page-resources"
		name="ConDec Release Notes Page Resources">
		<description>
			Provides the JavaScript files for the separate release note page of this
			plug-in.
		</description>
		<resource type="download" name="condec.release.note.page.js"
			location="/js/releasenotes/condec.release.note.page.js" />
